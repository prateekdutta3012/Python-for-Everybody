chocolate = 'Melody'
letter = chocolate[4]
print(letter)

chocolate='Melody'
for letter in chocolate:
  print(letter)

chocolate = 'Melody'
print(len(chocolate)

chocolate='Melody'
index=0
while index<len(chocolate):
  letter= chocolate[index]
  print(index,letter)
  index=index+1

chocolate='Kacha Mango Bite'
count=0
for letter in chocolate:
  if letter=='a':
    count=count+1
print(count)
